generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}



model old_data {
  id         String  @id @default(uuid())
  ID         Int
  Name       String?
  Phone      String?
  Gender     String?
  BloodGroup String?
  Year       String?
  Address    String?
  Sevakendra String?
}

model registration {
  id          String  @id @default(uuid())
  Id          Int
  Name        String?
  PhoneNumber String?
  AdharNumber String?
  Address     String?
  Bloodgroup  String?
  Sevakendra  String?
  DateofBirth String?
  Email       String?
  created_at  String?
}
